@prefix pdt:  <http://bimsparql.org/product#> .
@prefix ifcdoc: <http://www.buildingsmart-tech.org/ifc/IFC4/final#> .

pdt:hasWallLength  a      spin:MagicProperty , rdf:Property ;
        rdfs:subClassOf   spin:MagicProperties ;
        spin:body         [ a                   sp:Select ;
                            sp:resultVariables  ( [ sp:varName  "p"^^xsd:string ] ) ;
                            sp:where            ( [ sp:object     [ sp:varName  "p"^^xsd:string ] ;
                                                    sp:predicate  pdt:hasOverallLength ;
                                                    sp:subject    [ sp:varName  "arg1"^^xsd:string ]
                                                  ] )
                          ] ;
        spin:constraint   [ a              spl:Argument ;
                            spl:predicate  sp:arg1 ;
                            spl:valueType  rdfs:Resource
                          ] .